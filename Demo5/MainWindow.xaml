<Window x:Class="Demo5.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Demo5"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Canvas>
        <Button x:Name="buttonQueue" Content="线程不安全队列" Canvas.Left="220" Canvas.Top="57" Height="39" Width="147" Click="buttonQueue_Click" HorizontalAlignment="Center" VerticalAlignment="Top"/>
        <Button x:Name="buttonConcurrentQueue" Content="线程安全队列" Canvas.Left="220" Canvas.Top="127" Height="39" Width="147" Click="buttonConcurrentQueue_Click" HorizontalAlignment="Center" VerticalAlignment="Top"/>
        <Label Name="labelSerial" BorderThickness="1" BorderBrush="Black" Canvas.Left="220" Canvas.Top="211" HorizontalAlignment="Center" VerticalAlignment="Top" Width="337" Height="30"/>
        <Label Name="labelParallel" Canvas.Left="220" BorderThickness="1" BorderBrush="Black" Canvas.Top="282" Height="30" Width="344" HorizontalAlignment="Center" VerticalAlignment="Top"/>
        <Button x:Name="buttonRest" Content="重置" Canvas.Left="220" Canvas.Top="343" Height="32" Width="81" Click="buttonRest_Click" HorizontalAlignment="Center" VerticalAlignment="Top"/>
    </Canvas>
</Window>
